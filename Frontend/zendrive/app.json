{
  "expo": {
    "name": "zendrive",
    "slug": "zendrive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "sdkVersion": "53.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zendrive.app",
      "config": {
        "googleMapsApiKey": "AIzaSyBZpxciKcrgqvuxRePOCjHGMBttZ7Kdu4w"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zendrive.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBZpxciKcrgqvuxRePOCjHGMBttZ7Kdu4w"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
